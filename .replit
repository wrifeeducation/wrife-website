run = "npm run dev"
entrypoint = "index.js"
hidden = [".config", "package-lock.json", ".next", ".swc"]
modules = ["nodejs-22", "postgresql-16"]

[nix]
channel = "stable-24_11"

[env]
NEXT_TELEMETRY_DISABLED = "1"

[deployment]
build = ["npm", "run", "build"]
run = ["npm", "run", "start"]
deploymentTarget = "autoscale"

[[ports]]
localPort = 3000

[[ports]]
localPort = 3001

[[ports]]
localPort = 5000
externalPort = 5000

[[ports]]
localPort = 36653
externalPort = 80

[[ports]]
localPort = 39245
externalPort = 3002

[[ports]]
localPort = 39709
externalPort = 3000

[[ports]]
localPort = 41467
externalPort = 3001

[agent]
expertMode = true
integrations = ["javascript_openai_ai_integrations:1.0.0"]

[workflows]
runButton = "Project"

[[workflows.workflow]]
name = "Project"
mode = "parallel"
author = "agent"

[[workflows.workflow.tasks]]
task = "workflow.run"
args = "Next.js Dev Server"

[[workflows.workflow]]
name = "Next.js Dev Server"
author = "agent"

[workflows.workflow.metadata]
outputType = "webview"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "npm run dev"
waitForPort = 5000
